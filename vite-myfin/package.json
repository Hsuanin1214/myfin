{
  "name": "vite-myfin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run serve\" \"npm run watch-sass\" \"npm run watch-json\"",
    "serve": "vite",
    "watch-sass": "sass --watch assets/css/style.scss:assets/css/style.css",
    "watch-json": "json-server db.json --middlewares ./node_modules/json-server-auth",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "json-server": "^0.17.4",
    "json-server-auth": "^2.1.0",
    "sass": "^1.69.5",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2"
  }
}
